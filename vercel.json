{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "env": {
    "VITE_API_URL": "https://mentor-connect-server.vercel.app"
  },
  "build": {
    "env": {
      "VITE_API_URL": "https://mentor-connect-server.vercel.app"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html",
      "headers": {
        "Access-Control-Allow-Origin":  "*",
        "Access-Control-Allow-Methods": "GET,POST,PUT,DELETE"
      }
  ]
}
